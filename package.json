{
  "name": "bet-no-loss",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npx truffle compile",
    "test": "npx truffle test --stacktrace --compile-none",
    "build-doc": "npx solidity-docgen --solc-module solc-0.8 -i contracts/ -o doc/contracts/",
    "gas": "REPORT_GAS=true CI=true npm run test",
    "coverage": "npx truffle run coverage && (cat coverage/lcov.info | npx coveralls)",
    "deploy": "npx truffle deploy --network=ropsten",
    "start": "cd client/ && npm start",
    "watch": "npm run watch-test",
    "watch-test": "watch 'npm run test' contracts test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bet-no-loss/bet-no-loss.git"
  },
  "private": true,
  "keywords": [
    "ethereum",
    "smart-contract",
    "blockchain",
    "bet",
    "DeFi",
    "DApp",
    "solidity"
  ],
  "contributors": [
    {
      "name": "Eric Bouchut",
      "url": "http://EricBouchut.com"
    },
    {
      "name": "Tantely"
    },
    {
      "name": "Liedel Lam"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bet-no-loss/bet-no-loss/issues"
  },
  "homepage": "https://github.com/bet-no-loss/bet-no-loss#readme",
  "devDependencies": {
    "@codechecks/client": "^0.1.10",
    "@openzeppelin/test-helpers": "^0.5.10",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eth-gas-reporter": "^0.2.22",
    "ganache-cli": "^6.12.2",
    "js-object-pretty-print": "^0.3.0",
    "luxon": "^1.26.0",
    "solc-0.8": "npm:solc@^0.8.3",
    "solidity-coverage": "^0.7.16",
    "solidity-docgen": "^0.5.11",
    "truffle": "^5.3.3",
    "watch": "^1.0.2",
    "web3-eth": "^1.3.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.0.0",
    "@truffle/hdwallet-provider": "^1.2.6",
    "dotenv": "^8.2.0"
  }
}
